<script type="text/javascript">

    jQuery(document).ready(function(){
        jQuery(".chiudi2").click(function(){
            jQuery('#overlay2').fadeOut('slow');
            jQuery('#misura_div').hide();
        });
        jQuery("#overlay2").click( function(){
            jQuery(this).fadeOut('slow');
            jQuery('#misura_div').hide();
        });

        if (jQuery("#codiceTaglia").val()==""){
            jQuery("#misura_div_link").hide();
        }

        jQuery("#misura_div_link").click(function(){

            jQuery('#overlay2').fadeIn('fast');

            jQuery('#misura_div').fadeIn('slow');

            codiceTaglia=jQuery("#codiceTaglia").val();
            jQuery("#sizeChartSel").val(codiceTaglia);

            changeSizeTable(codiceTaglia);

            jQuery('#misura_div').css("top", Math.max(0, ((jQuery(window).height() - jQuery('#misura_div').outerHeight()) / 2) ) + "px");
            jQuery('#misura_div').css("left", Math.max(0, ((jQuery(window).width() - jQuery('#misura_div').outerWidth()) / 2) ) + "px");
        });

    })

    function fireEvent(element,event)
    {
        if (document.createEventObject)
        {
            // dispatch for IE
            var evt = document.createEventObject();
            return element.fireEvent('on'+event,evt);
        }
        else
        {
            // dispatch for firefox + others
            var evt = document.createEvent("HTMLEvents");
            evt.initEvent(event, true, true );
            return !element.dispatchEvent(evt);
        }
    }

    function setDefaultConfigOptions()
    {
        if (spConfigIndex >= spConfig.settings.length)
        {
            return; // stop
        }

        spConfig.settings[spConfigIndex].selectedIndex = 1;
        var obj = spConfig.settings[spConfigIndex];

        ++spConfigIndex;
        jQuery(".taglia_singola[data-value="+obj.value+"]").addClass('taglia_singola_selected');

        Event.observe(obj,'change',function(){});
        fireEvent(obj,'change');

        window.setTimeout("setDefaultConfigOptions()", 1); // Add a small delay before moving onto the next option
    }

    var spConfigIndex = 0;
    Event.observe(window, 'load', function() {
        replaceDropDowns();
        setDefaultConfigOptions();
    });

    function replaceDropDowns() {
        jQuery('.taglia_singola').remove();
        jQuery(".super-attribute-select").each(function() {
            var drop_down = jQuery(this);
            drop_down.hide();
            drop_down.find("option[value!='']").each(function() {
                var option = jQuery(this);
                jQuery("<div>", {
                    text: option.text(),
                    class: 'taglia_singola',
                    'data-id': drop_down.attr('id'),
                    'data-name': drop_down.attr('name'),
                    'data-value': option.val(),
                    'data-label': option.text(),
                    click: function() {
                        drop_down.val(option.val());
                        var obj = drop_down.get();
                        Event.observe(obj[0],'change',function(){});
                        fireEvent(obj[0],'change');
                        replaceDropDowns();
                        jQuery(".super-attribute-select").each(function() {
                            if(jQuery(this).val()) {
                                jQuery(".taglia_singola[data-value="+jQuery(this).val()+"]").addClass('taglia_singola_selected');

                            }
                        });

                        return false;
                    }
                }).appendTo(drop_down.parent());
            })
        });
    }


    function changeSizeTable(valore){
        tabelle=jQuery(".size_table");
        for (i=0; i<tabelle.lenght; i++){
            jQuery(tabelle[i]).hide();
        }

        jQuery('.size_table').each(function(){
            jQuery(this).hide();
        })

        largh=jQuery("#size_table_"+valore).width();
        jQuery("#size_table_"+valore).css("width",largh+"px");
        jQuery(".chiudi2").css("margin-left",largh+"px");
        largh=largh+30;
        jQuery("#misura_div").css("width",largh+"px");
        jQuery("#size_table_"+valore).show();
    }
</script>


